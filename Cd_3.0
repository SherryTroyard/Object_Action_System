void loadAUC()
{
    ifstream in;
    in.open("Obj.txt");
    for(int i = 0; i < N; i++){
        in >> AUC[i].name;
        in >> AUC[i].Uord;
        in >> AUC[i].sta;
        in >> AUC[i].type;
        in >> AUC[i].dcr;
        in >> AUC[i].price;
        in >> AUC[i].ordin;
        in >> AUC[i].whe;
        in >> AUC[i].time[0];
        in >> AUC[i].time[1];
        in >> AUC[i].time[2];
        in >> AUC[i].time[3];
        in >> AUC[i].time[4];
        in >> AUC[i].time[5];
        in >> AUC[i].buyer;
        in >> AUC[i].fin;
        qua = max(qua, AUC[i].ordin);
        if(AUC[i].name.empty()) break;
    }
    in.close();
}

void outAUC()
{
    ofstream out;
    out.open("Obj.txt", ios::trunc);
    out.close();
    out.open("Obj.txt", ios::app);
    for(int i = 0; i < N; i++) {
        if(AUC[i].name.empty())
            break;
        else
            out << AUC[i].name << " " << AUC[i].Uord << " " << AUC[i].sta << " " << AUC[i].type
                << " " << AUC[i].dcr << " " << AUC[i].price <<  " " << AUC[i].ordin <<  " " << AUC[i].whe << " "
                << AUC[i].time[0] << " " << AUC[i].time[1] << " " << AUC[i].time[2] << " " << AUC[i].time[3]
                << " " << AUC[i].time[4] << " " << AUC[i].time[5] << " " << AUC[i].buyer << " " << AUC[i].fin << endl;
    }
    out.close();
}

